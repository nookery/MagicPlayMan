      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(git restore:*)",
      "Bash(git rm:*)",
      "Bash(mkdir:*)",
      "Bash(git mv:*)",
